{
  "Milvus": {
    "type": "object",
    "description": "Milvus配置",
    "items": {
      "islite": {
        "type": "bool",
        "description": "是否使用lite模式",
        "hint": "lite模式只需要填写保存地址这一项",
        "default": true
      },
      "host": {
        "type": "string",
        "description": "服务器地址",
        "hint": "默认为localhost"
      },
      "port": {
        "type": "string",
        "description": "端口号",
        "hint":"默认为19530"
      },
      "user": {
        "type": "string",
        "description": "用户名"
      },
      "password": {
        "type": "string",
        "description": "密码"
      },
      "lite_path": {
        "type": "string",
        "description": "lite模式保存地址",
        "hint": "data/your_path",
        "default": "data/astrbot_plugin_cyber_archaeology"
      }
    }
  },
  "plugin_conf":{
    "type": "object",
    "description": "插件配置",
    "items":{
      "top_k": {
        "type": "int",
        "description": "返回结果数量",
        "default": 5
      }
    }
  }
}