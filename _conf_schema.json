{
  "Milvus": {
    "type": "object",
    "description": "Milvus配置",
    "items": {
      "islite": {
        "type": "bool",
        "description": "是否使用lite模式"
      },
      "host": {
        "type": "string",
        "description": "服务器地址，默认为localhost"
      },
      "port": {
        "type": "string",
        "description": "端口号，默认为19530"
      },
      "user": {
        "type": "string",
        "description": "用户名，默认为空"
      },
      "password": {
        "type": "string",
        "description": "密码，默认为空"
      }
    }
  },
  "plugin_conf":{
    "type": "object",
    "description": "插件配置",
    "items":{
      "top_k": {
        "type": "int",
        "description": "返回结果数量",
        "default": 5
      }
    }
  }
}